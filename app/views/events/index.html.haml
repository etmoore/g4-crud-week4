%table.table
  %thead
    %tr
      %th Date
      %th Location
      %th Description
      %th Capacity
      %th Requires id?
  %tbody
    - @tasks.each do |task|
      %tr
        %td=task.date
        %td=task.location
        %td=task.description
        %td=task.capacity
        %td=task.requires_id
